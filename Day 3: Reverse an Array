class Solution {
    public void reverseArray(int arr[]) {
        
        int n = arr.length;
        int first = 0;
        int last = n-1;
        
        while(first<last)
        {
            
            int temp = arr[last];
            arr[last] = arr[first];
            arr[first] = temp;
            first++;
            last--;
            
        }
        
    }
}
