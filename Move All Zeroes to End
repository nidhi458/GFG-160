class Solution {
    void pushZerosToEnd(int[] arr) {
        
        int last = arr[arr.length - 1];
        for(int i = 0;i<arr.length;i++)
        {
            
            if(arr[i]==0 && arr[i+1]==0)
            {
                continue;
            }
            if(arr[i]!=0)
            {
               arr[i+1] = arr[i];
               
            }
            
          }
        
     }
}
